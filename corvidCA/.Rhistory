library(auk)
library(readr)
library(tidyverse)
auk_set_ebd_path("~/Desktop/BirdPresenceAbsence/corvidCA", overwrite=TRUE)
ebd<-read_tsv("ebd.txt")
smp<-read_tsv("sampling.txt")
f_ebd <- "ebd_corvid.txt"
